<IfModule mod_rewrite.c>
  RewriteEngine On

  # DÃ©finir la racine du projet (ici le dossier appliLivraison)
  RewriteBase /TP3-Menu-Pizzeria/

  # Ne pas rediriger si le fichier ou dossier existe
  RewriteCond %{REQUEST_FILENAME} -f [OR]
